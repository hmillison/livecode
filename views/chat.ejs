    <body>
        <nav class="navbar navbar-default" role="navigation">
            <h2> Live Code </h2>
        </nav>

        <div class="container">
          <div class="col-md-6 camera">
                <div class="camera-box">
                        <video height="300" id="localVideo"></video>
                </div>
          </div>
            <div class="col-md-6 camera">
                 <div class="camera-box">
                        <div id="remotesVideos"></div>
                </div>
            </div>

        </div>

        <div>
              <div id="wrapper">
                    <div id="sidebar-wrapper">
                         <ul class="sidebar-nav">
                            <li></li>
